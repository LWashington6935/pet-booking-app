{
  "name": "pet-booking-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest run",
    "test:ui": "vitest",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": "vercel --prod",
    "deploy:storybook": "npm run build-storybook && vercel --prod storybook-static",
    "build:ui": "npm --workspace @pet/ui run build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "8.1.10",
    "@storybook/react": "8.1.10",
    "@storybook/react-vite": "8.1.10",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.11.25",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@vitejs/plugin-react": "4.3.1",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "husky": "9.0.11",
    "jsdom": "24.1.0",
    "lint-staged": "15.2.7",
    "playwright": "1.46.0",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3",
    "tsup": "8.1.0",
    "typescript": "5.4.5",
    "vercel": "34.2.7",
    "vitest": "1.6.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": "prettier --write"
  },
  "workspaces": [
    "packages/*"
  ]
}
